{
    "name": "quick-notes",
    "displayName": "Quick Notes",
    "description": "This extension enables you to take quick notes",
    "version": "1.5.0",
    "license": "MIT",
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "url": "https://github.com/sunliangqin/VSCodeQuickNotes.git"
    },
    "publisher": "Liangqin",
    "activationEvents": [
        "onCommand:quickNotes.toggle"
    ],
    "main": "./out/extension.js",
    "icon": "assets/icon.png",
    "contributes": {
        "commands": [
            {
                "command": "quickNotes.toggle",
                "title": "Toggle Note File"
            }
        ],
        "keybindings": [
            {
                "command": "quickNotes.toggle",
                "key": "alt+`",
                "mac": "alt+tab"
            }
        ],
        "configuration": {
            "title": "Quick Notes",
            "properties": {
                "quickNotes.fileName": {
                    "type": "string",
                    "default": "./.vscode/notes.txt",
                    "description": "Note file path."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.67",
        "@types/vscode": "^1.50.0",
        "tslint": "^5.20.1",
        "typescript": "^3.9.7"
    }
}
