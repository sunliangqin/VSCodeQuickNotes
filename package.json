{
    "name": "quick-notes",
    "displayName": "Quick Notes",
    "description": "This extension enables you to take notes in vscode easily.",
    "version": "1.4.0",
    "license": "MIT",
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "url": "https://github.com/sunliangqin/VSCodeQuickNotes.git"
    },
    "publisher": "Liangqin",
    "activationEvents": [
        "onCommand:quick-notes.toggle"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "quick-notes.toggle",
                "title": "Toggle note file"
            }
        ],
        "keybindings": [
            {
                "command": "quick-notes.toggle",
                "key": "alt+`",
                "mac": "alt+tab"
            }
        ],
        "configuration": {
            "title": "Quick Notes",
            "properties": {
                "quickNotes.fileName": {
                    "type": "string",
                    "default": "./.vscode/notes.txt",
                    "description": "Note file path."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.42",
        "@types/vscode": "^1.45.1",
        "tslint": "^5.20.1",
        "typescript": "^3.9.3"
    }
}
