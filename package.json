{
    "name": "quick-notes",
    "displayName": "Quick Notes",
    "description": "This extension enables you to take notes in vscode easily.",
    "version": "1.4.0",
    "license": "MIT",
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "url": "https://github.com/sunliangqin/VSCodeQuickNotes.git"
    },
    "publisher": "Liangqin",
    "activationEvents": [
        "onCommand:quickNotes.toggle"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "quickNotes.toggle",
                "title": "Toggle Quick Notes"
            }
        ],
        "keybindings": [
            {
                "command": "quickNotes.toggle",
                "key": "alt+`",
                "mac": "alt+tab"
            }
        ],
        "configuration": {
            "title": "Quick Notes",
            "properties": {
                "quickNotes.fileName": {
                    "type": "string",
                    "default": "./.vscode/notes.txt",
                    "description": "Quick notes file name."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.38",
        "@types/vscode": "^1.31.0",
        "tslint": "^5.20.1",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    }
}
